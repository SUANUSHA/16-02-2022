#include <stdio.h>
int main()
{
  int a=4,b=8,c=3,d=9,z;
  z=a++ + ++b*c-- - --d;
  printf("a=%d,b=%d,c=%d,d=%d,z=%d\n",a,b,c,d,z);
  a=9,b=15,c=16,d=12;
  int e,f;
  e=!(a<b||b<c);
  f=(a>b)?a-b:b-a;
  printf("e=%d,f=%d\n",e,f);
  a=14;
  a=a%5;
  b=a/3;
  c=a/5%3;
  printf("a=%d,b=%d,c=%d\n",a,b,c);
  a=(1,2,3);
  printf("a=%d\n",a);
  a=1,2,3;
  printf("a=%d\n",a);
  printf("a=%d,b=%d,c=%d\n",a,b,c);
  printf("%ld\n",sizeof(a)<5+b++-++c);
  int i=10;
  char ch='a';
  int sum;
  sum=i+ch; // implicit conversion of c from char to intger type
  printf("value of sum=%d\n",sum);
  double x = 1.2;
  int y = (int)x + 1;
  printf("y = %d\n", y);
  return 0;
} 


