#include<stdio.h>
int main()
{
  int arr[5]={1,2,3,4,5};
  int *p=arr;
  printf("%d %d %d %d %d\n",*p,*p+1,*p+2,*p+3,*p+4);
  printf("%d %d %d %d %d\n",*arr,*(1+arr),*(2+arr),*(3+arr),*(4+arr));
  printf("%d %d %d %d %d\n",*p,*(arr+1),*(arr+2),*(arr+3),*(arr+4));
  printf("%d %d %d %d %d\n",arr[0],1[arr],2[arr],3[arr],4[arr]);
  printf("%d %d %d %d %d\n",arr[0],arr[1],arr[2],arr[3],arr[4]);
  char str[] = "My cat's name is Wiggles.";
  printf("%c %c %c %c\n", str[5], *(str + 5), *(5 + str), 5[str]);
  printf("%c %c %c %c\n", str[0], *(str + 0), *(0 + str), 0[str]);
  printf("%c %c %c %c\n", str[1], *(str + 1), *(1 + str), 1[str]);
  return 0;
}
